{
    "name": "kravanh-eco/laravel-cmd",
    "description": "Laravel cmd for internal use",
    "license": "license",
    "authors": [
        {
            "name": "ketsakda",
            "email": "ketsakda@kravnah.com",
            "homepage": "https://kravanh.com"
        }
    ],
    "homepage": "https://github.com/kravanh-eco/laravel-cmd",
    "keywords": ["Laravel", "LaravelCmd"],
    "require": {
        "illuminate/support": "~7|~8",
        "spatie/laravel-view-models": "^1.3",
        "spatie/laravel-query-builder": "^3.4",
        "spatie/laravel-model-states": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "KravanhEco\\LaravelCmd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KravanhEco\\LaravelCmd\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KravanhEco\\LaravelCmd\\LaravelCmdServiceProvider"
            ],
            "aliases": {
                "LaravelCmd": "KravanhEco\\LaravelCmd\\Facades\\LaravelCmd"
            }
        }
    }
}
